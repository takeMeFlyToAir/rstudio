         <tr>
            <td class="{res.styles.NW}"></td>
            <td class="{res.styles.N}"></td>
            <td class="{res.styles.NE}"></td>
         </tr>
         <tr>
            <td class="{res.styles.W}"></td>
            <td class="{res.styles.C}">
