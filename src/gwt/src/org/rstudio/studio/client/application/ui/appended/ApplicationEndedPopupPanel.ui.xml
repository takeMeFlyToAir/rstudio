            <td class="{res.styles.E}"></td>
         </tr>
         <tr>
            <td class="{res.styles.SW}"></td>
            <td class="{res.styles.S}"></td>
            <td class="{res.styles.SE}"></td>
         </tr>
      </table>
   </g:HTMLPanel>

</ui:UiBinder>
